# 导出文件 #
proc export date= file_name  outfile= "&path/name/filename.csv" dbms=csv replace;     # data=指定要输出的文件，outfile=""指定输出文件路径，名称和格式 #
run;

# 使用library功能将数据表导入到指定excel workbook中 #
libname outxl xlsx "&path/excel文件路径和文件名";           # 讲文件输出到指定Excel文件中的worksheet #
data outxl.file1;                                        # 建立在outxl文件中的worksheet #
  set file1;
run;

libname outxl clear;                                     # 确定将建立的worksheet导入到outxl文件中 #

# 导出PDF文件 #
ods pdf file="&path/文件路径/文件名.pdf" 
        pdftoc=1 startpage=no;                           # pdftoc=1设置为仅显示第一级的书签，不展开;startpage=no设置为不分页 #

ods proclable 'The title of file1';                      # 设置书签标题 #
title 'The title of file1';
proc print data=file1;
run;

ods pdf close;

# 将每个report导入到不同的Excel workbook中 #
ods excel file="&path/filename.xlxs"
          options(embedded_title="yes")                 # embedded_title="yes"打开设置worksheet开关 #
          style= analysis;                              # 改变外观 #
ods excel options(sheet_name=" Name of first worksheet"); # 设置具体worksheet名，对于不同的worksheet的改名，本行都需要重新写#
title " Name of first worksheet";
proc freq data=first_worksheet;
run;

ods excel options(sheet_name=" Name of Second worksheet"); # 另一个worksheet名字设置 #
title " Name of Second worksheet";
proc freq data=Second_worksheet;
run;

ods excel close;



